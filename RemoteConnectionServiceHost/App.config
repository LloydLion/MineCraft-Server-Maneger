<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>

    <services>
      <service name="MineCraft_Server_Maneger.Remote.RemoteConnectionService">
        <endpoint address="" binding="basicHttpBinding" contract="MineCraft_Server_Maneger.Remote.IRemoteConnectionService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>

        <endpoint address="" binding="netTcpBinding" contract="MineCraft_Server_Maneger.Remote.IRemoteConnectionService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>

        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />

        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/MineCraft_Server_Maneger.Remote/RemoteConnectionService/" />
            <add baseAddress="net.tcp://localhost:8734/Design_Time_Addresses/MineCraft_Server_Maneger.Remote/RemoteConnectionService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>